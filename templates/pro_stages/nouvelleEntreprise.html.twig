{% extends 'base.html.twig' %}

{% block titresPage %}
<h1><center>Creer une nouvelle entreprise</center></h1>
{% endblock %}
{% block contenu %}
<center>
<div class="p-5">
	<div class="p-5 border bg-light">
	{{ form_start(vueFormulaire, { 'attr' : {'novalidate':'novalidate'}} ) }}
		{{ form_row(vueFormulaire.nom) }}
		{{ form_row(vueFormulaire.activite) }}
		{{ form_row(vueFormulaire.adresse) }}
		{{ form_row(vueFormulaire.lienSiteWeb) }}
		<button type="submit"> Enregistrer </button>
	{{ form_end(vueFormulaire) }}
</div>
</div>
</center>
{% endblock %}